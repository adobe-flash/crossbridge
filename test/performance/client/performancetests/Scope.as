the test:		protected static const cA:Number=10;		protected static const cB:Number=5;		protected static var sA:Number;		protected static var sB:Number;		protected var iA:Number;		protected var iB:Number;			// Initialization:		public function Scope() {			iA = sA = cA;			iB = sB = cB;		}			// Public getter / setters:			// Public Methods:		// tare method that will be run to calculate the base time required to run the loop		// so it can be subtracted from the other test results.		public function tare():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = 50;			}		}				public function literals():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = 10.0*5.0+10.0+5.0/10.0;			}		}			public function local():void {			var lA:Number = cA;			var lB:Number = cB;			for (var i:uint=0; i<loops; i++) {				var c:Number = lA*lB+lA+lB/lA;			}		}				public function instance():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = iA*iB+iA+iB/iA;			}		}				public function instanceExplicit():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = this.iA*this.iB+this.iA+this.iB/this.iA;			}		}				public function staticVars():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = sA*sB+sA+sB/sA;			}		}				public function staticExplicit():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = Scope.sA*Scope.sB+Scope.sA+Scope.sB/Scope.sA;			}		}				public function constants():void {			for (var i:uint=0; i<loops; i++) {				var c:Number = cA*cB+cA+cB/cA;			}		}			// Protected Methods:			}	}