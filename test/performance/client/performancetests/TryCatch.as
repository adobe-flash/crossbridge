		public function TryCatch() {		}			// tare method that will be run to calculate the base time required to run the loop		// so it can be subtracted from the other test results.		public function tare():void {			var isNull:Object = null;			var isNotNull:Object = {};			for (var i:uint=0; i<loops; i++) {			}		}				public function tryCatch():void {			var isNull:Object = null;			var isNotNull:Object = {};			for (var i:uint=0; i<loops; i++) {				try { isNull.x = 3; } catch (e:Error) {}				try { isNotNull.x = 3; } catch (e:Error) {}			}		}				public function ifNull():void {			var isNull:Object = null;			var isNotNull:Object = {};			for (var i:uint=0; i<loops; i++) {				if (isNull) { isNull.x = 3; }				if (isNotNull) { isNotNull.x = 3; }			}		}				// Protected Methods:			}	}