ed var refF:Function;			// Initialization:		// because tests are instance based, you can use the constructor to set things up		// prior to your tests running.		public function Functions():void {			var b:Number = 2;			var c:String = "this is a test";			anonymousRefF = function() {				var a:Number = 10;			}			refF = sampleF;		}			// Public getter / setters:			// Public Methods:		// tare method that will be run to calculate the base time required to run the loop		// so it can be subtracted from the other test results.		public function tare():void {			for (var i:uint=0; i<loops; i++) {				var a:Number = 10;			}		}				public function anonymous():void {			for (var i:uint=0; i<loops; i++) {				(function() { var a:Number = 10; })();			}		}				public function anonymousRef():void {			for (var i:uint=0; i<loops; i++) {				anonymousRefF();			}		}				public function method():void {			for (var i:uint=0; i<loops; i++) {				sampleF();			}		}				public function reference():void {			for (var i:uint=0; i<loops; i++) {				refF();			}		}			// Protected Methods:		// note that protected/private methods aren't tested.		protected function sampleF():void {			var a:Number = 10;		}			}	}